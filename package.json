{
  "name": "jennycodes",
  "private": true,
  "engines": {
    "node": "22.11.0"
  },
  "scripts": {
    "build": "esbuild app/javascript/application.js --bundle --sourcemap --format=esm --outdir=app/assets/builds --public-path=/assets",
    "build:css": "sass ./app/assets/stylesheets/application.sass.scss:./app/assets/builds/application.css ./app/assets/stylesheets/advent.css.scss:./app/assets/builds/advent.css --no-source-map --load-path=node_modules --quiet-deps"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "esbuild": "^0.25.11",
    "jquery": "^3.7.1",
    "jquery-lazy": "^1.7.11",
    "sass": "^1.93.2"
  }
}
